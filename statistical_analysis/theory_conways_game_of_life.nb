5ddd6c4b05f8"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"numberOfRecursions", " ", "=", " ", "20"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"initialDensities", " ", "=", " ", 
   RowBox[{"Range", "[", 
    RowBox[{"0.", ",", "1.", ",", "0.005"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"densityEvolution", "=", 
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"RecurrenceTable", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"\[Rho]", "[", 
            RowBox[{"n", "+", "1"}], "]"}], "\[Equal]", 
           RowBox[{"updateFunc", "[", 
            RowBox[{"\[Rho]", "[", "n", "]"}], "]"}]}], ",", 
          RowBox[{
           RowBox[{"\[Rho]", "[", "1", "]"}], "\[Equal]", "#"}]}], "}"}], ",",
         "\[Rho]", ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", "1", ",", "numberOfRecursions"}], "}"}]}], "]"}], 
      "&"}], ",", "initialDensities"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"finalDenisities", "=", 
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"#", "[", 
       RowBox[{"[", 
        RowBox[{"-", "1"}], "]"}], "]"}], "&"}], ",", "densityEvolution"}], 
    "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.828439928657658*^9, 3.82843994000336*^9}, {
