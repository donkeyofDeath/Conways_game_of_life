18daec73830c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"updateFunc", "[", "p_", "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"Binomial", "[", 
     RowBox[{"8", ",", " ", "2"}], "]"}], " ", "*", " ", 
    RowBox[{"p", "^", "3"}], " ", "*", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", " ", "-", " ", "p"}], ")"}], "^", "6"}]}], " ", "+", " ", 
   RowBox[{
    RowBox[{"Binomial", "[", 
     RowBox[{"8", ",", "3"}], "]"}], " ", "*", " ", 
    RowBox[{"p", "^", "3"}], " ", "*", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", " ", "-", " ", "p"}], ")"}], "^", "5"}]}]}]}]], "Input",
 CellChangeTimes->{{3.8284397982341537`*^9, 3.82843981765504*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"939564bd-42ca-4582-84e4-8795518ffd44"],

